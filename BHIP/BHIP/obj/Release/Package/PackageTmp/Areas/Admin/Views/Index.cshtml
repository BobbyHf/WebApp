<!-- CSS Styles -->
<style type="text/css">
    /* layout */
    * {
        margin: 0;
        padding: 0;
    }

    a:link {
        color: #ffffff;
        font-size: 20px;
    }

    a:visited {
        color: #ffffff;
        font-size: 20px;
    }

    a:hover {
        color: #ff6a00;
        font-size: 20px;
    }
    /* white background */
    #container {
        margin: 0 auto;
        width: 900px;
        height: auto;
    }

    /* look and feel */
    .targetBorder {
        border: dotted #ccc 1px;
        padding: 10px;
    }
    /* Size of each box - 6 in total */
    .draggable {
